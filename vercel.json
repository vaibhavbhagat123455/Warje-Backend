{
    "version": 2,
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/auth/signup",
        "dest": "/api/auth/signup.js"
      },
      {
        "src": "/api/auth/login",
        "dest": "/api/auth/login.js"
      },
      {
        "src": "/api/profile",
        "dest": "/api/profile.js"
      }
    ]
  }  